{
  "name": "llm-service",
  "version": "1.0.0",
  "description": "Tiger Tickets LLM-driven booking service for natural language event search and ticket purchase",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest __tests__/unit",
    "test:integration": "jest __tests__/integration"
  },
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@ai-sdk/groq": "^2.0.26",
    "ai": "^5.0.82",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "supertest": "^6.3.4"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "controllers/**/*.js",
      "models/**/*.js",
      "routes/**/*.js",
      "!**/*.test.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 20,
        "lines": 50,
        "statements": 50
      }
    },
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ]
  }
}
